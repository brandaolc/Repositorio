algoritmo "calculando area de um retangulo base x altura"

var
   largura, altura, area : inteiro


inicio

 escreval("digite o valor da largura do retangulo")
 leia(largura)

 escreval("digite o valor da altura do retangulo")
 leia(altura)

 area := altura * largura

   escreval("a area do retangulo é" , area:5:2)


fimalgoritmo